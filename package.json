{
  "name": "@eigr/spawn-sdk",
  "version": "0.5.1",
  "description": "Spawn Actor model framework implementation for NodeJS",
  "main": "_build/index.js",
  "types": "_build/index.d.ts",
  "scope": "@eigr",
  "private": false,
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "_build",
    "package.json",
    "yarn.lock",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "tsc": "tsc",
    "proto-gen": "protoc --ts_out ./src/protos/ --proto_path protos protos/**/*.proto",
    "proto-gen-spawn": "protoc --ts_out ./src/protos/eigr/functions/protocol/actors --proto_path protos/eigr/functions/protocol/actors protos/eigr/functions/protocol/actors/*.proto",
    "start": "ts-node --esm --require tsconfig-paths/register index.ts",
    "run-benchmark": "ts-node --esm --require tsconfig-paths/register benchmark/benchmark.ts",
    "test": "jest --forceExit --runInBand",
    "format": "prettier --config .prettierrc '{src,test}/**/*.ts' --write"
  },
  "author": "eigr",
  "license": "ISC",
  "_moduleAliases": {
    "@protos": "dist/protos"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/node-fetch": "^2.6.2",
    "@types/stoppable": "^1.1.1",
    "benny": "^3.7.1",
    "jest": "^29.0.3",
    "prettier": "^2.8.2",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.8.1",
    "ts-proto": "^1.126.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@protobuf-ts/plugin": "^2.7.0",
    "node-fetch": "^2.6.1",
    "stoppable": "^1.1.0"
  }
}
